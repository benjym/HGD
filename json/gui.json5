    outlet_width : 0.01,  // m
    sigma_charge : 0.1,  // relative width of charge zone
    charge_rate : 0.0005,  // rate of charge zone filling (kg/s)
    outlet_rate : 0.1,  // rate of outlet opening
    

    s_m : 45e-6, // minimum particle size (m)
    s_M : 150e-6, // maximum particle size (m)
    gsd_mode : 'bi',
    nu_cs : 0.5,  // critical density above which motion stops

    // initial conditions
    IC_mode : "top",  // just leave empty space at the top
    nu_fill : 0.5,
    fill_ratio : 0.25,

    // temporal stuff
    t_f : 1000,  // final time
    
    // saving
    save_inc : 10,
    // saves : 100,  // how often to save
    // videos : ['nu', 's', 'stress'],  // what to save videos of
    // save : ['outlet'],

    // testing area
    // close_voids : false,
    // vectorize : false
    view : 'nu',

    motion_model : 'd2q4_SA_array', // 'd2q4_slow', 'd2q4_array' or 'd2q9_slow'
    inertia : false,
    elutriation : false,
    advection_model : 'freefall', // 'average_size', 'freefall' or 'stress'

    cycles : [
        { mode: "charge",
        "-45" : 0.2,
        "-53" : 0.2,
        "-76" : 0.2,
        "-106" : 0.2,
        "+150" : 0.2,
        mass : 200
        },
        { mode: "discharge",
        "-45" : 0.2,
        "-53" : 0.2,
        "-76" : 0.2,
        "-106" : 0.2,
        "+150" : 0.2,
        mass : 200
        },
    ],
    cycle_forever : true,
    mask_s_bar : false,

    gui : {
        // nx : {min: 10, max: 100, step: 1, dtype: 'int', 'title': 'Resolution in grid points'},
        nx : {dtype: 'select', options: [20, 40, 80], labels: ['Low', 'Medium', 'High'], 'title': 'Mesh quality'},
