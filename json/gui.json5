{
    // resolution
    // nx : 20, // width
    nx : 20,
    aspect_ratio_y : 2,  // ny = aspect_ratio * nx
    aspect_ratio_m : 1,  // nm = aspect_ratio * nx
    // nm : 40,
    // nm : ,

    // geometry
    H : 1, // m
    
    // material properties
    // alpha : [1.0,10.0,100.0],
    alpha : 2.0,
    // alpha : [0.5,1.0,2.0],  
