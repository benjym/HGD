    s_m : 0.001, // minimum particle size (m)
    s_M : 0.001, // maximum particle size (m)
    nu_cs : 0.5,
    // alpha : 0.003,
    
    // initial condition:
    fill_ratio : 0.25,
    nu_fill : 0.5,
    IC_mode : "column",
    
    // saving
    save_inc : 1000,   // how many saves to make
    t_f : null,  // s - cant use null because of repose_angle=0 :(
    
    plot : ['nu', 'stress'],
    videos : ['nu','stress'],
    save : ['nu'],
    mask_s_bar : false,

