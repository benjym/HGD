{
    // resolution
    nx : 5, // number of cells in x
    ny : 31, // number of cells in y
    nm : 50,  // number of cells in microstructural direction

    // geometry
    H : 1, // m
    // boundaries : ["slope_everywhere_to_fraction"],
    // boundaries : ["slope_everywhere_to_empty"],
    boundaries : ["slope_everywhere_to_first"],

    cyclic_BC : true,
    cyclic_BC_y_angle : [20,21,22,23,24,25,26,27],
    // void_introduction_rate : [0.0001,0.0003,0.0005,0.001,0.003,0.005,0.01,0.03,0.05,0.1,0.2,0.3,0.5,1.0,1.5,2.0,2.5,3.0],
    // void_introduction_rate : [0.01,0.03,0.05,0.1,0.2,0.3,0.5,1.0,1.5,2.0,2.5,3.0],
    
    // material properties
    s_m : 0.001, // minimum particle size (m)
    s_M : 0.01, // maximum particle size (m)
    gsd_mode : 'bi',
    nu_cs : 0.5,
    alpha : 4,
    repose_angle : 20, // (degrees)
    P_stab: 0.5,

    masks : ["slope"],
    motion_model : 'd2q4_cpp',
    advection_model : 'stress',
    inertia: false,
    
    // initial conditions
    // IC_mode : "bottom",  // just leave empty space at the top
    IC_mode : "slope",
    fill_ratio : 0.45,
    nu_fill : 0.5,

    // temporal stuff
    t_f : 10.0,  // final time
    time_average: true,
    
    // saving
    save_inc : 500,  // how often to save
    plot : ['nu', 's', 'U_mag', 'chi'],
    videos : ['nu', 's', 'U_mag', 'chi'],
    save : ['nu', 's', 'velocity', 'chi'],

    max_workers: -1,

    // testing area
    // close_voids : false,
}